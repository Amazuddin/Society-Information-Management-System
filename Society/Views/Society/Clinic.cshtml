            <a style="text-decoration: none;" href="@Url.Action("DoctorInformation", "Society")">
                <div class="po3">
                    <h1 class="rounded-circle po4-img"><i class="fas fa-user-md"></i></h1>
                    <div class="po5">
                        <h2>
                            DOCTORS
                        </h2>

                    </div>

                </div>
            </a>
        </div>
        <div class="col-md-4">
            <a style="text-decoration: none;" href="@Url.Action("Appointment", "Society")">
                <div class="po3">
                    <h1 class="rounded-circle po4-img"><i class="fas fa-calendar-check"></i></h1>
                    <div class="po5">
                        <h2>
                            APPOINTMENT
                        </h2>

                    </div>

                </div>
            </a>
        </div>
        <div class="col-md-2"></div>
    </div>

   
</div>
    
       

