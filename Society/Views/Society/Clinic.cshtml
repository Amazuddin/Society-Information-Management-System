    <meta name="viewport" content="width=device-width" />
<title>Clinic</title>
<link href="~/Content/cliicstyle.css" rel="stylesheet" />
<style>
    .carousel-inner img {
        width: 100%;
        height: 550px;
    }
</style>
<script type="text/javascript">
    $('.carousel').carousel({
        interval: 1000
    }) </script>
<body>
<div id="demo" class="carousel slide" data-ride="carousel">

    <!-- Indicators -->
    <ul class="carousel-indicators">
        <li data-target="#demo" data-slide-to="0"></li>
        <li data-target="#demo" data-slide-to="1"></li>
        <li data-target="#demo" data-slide-to="2"></li>
        <li data-target="#demo" data-slide-to="3"></li>
        <li data-target="#demo" data-slide-to="4"></li>
    </ul>

    <!-- The slideshow -->
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/Picture/1.jpg" alt="Los Angeles">
        </div>
        <div class="carousel-item">
            <img src="~/Picture/2.jpg" alt="Chicago">
        </div>
        <div class="carousel-item">
            <img src="~/Picture/3.jpg" alt="New York">
        </div>
        <div class="carousel-item">
            <img src="~/Picture/4.jpg" alt="New York">
        </div>
        <div class="carousel-item">
            <img src="~/Picture/5.jpg" alt="New York">
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="carousel-control-prev" href="#demo" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </a>
    <a class="carousel-control-next" href="#demo" data-slide="next">
        <span class="carousel-control-next-icon"></span>
    </a>
</div>


<div class="container">
    <div class="po1">
        <center>
            <strong id="po2">Services</strong>
        </center>
    </div><br>

    <div class="row">

        <div class="col-md-2"></div>
